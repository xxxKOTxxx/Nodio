@import "variables.styl"
.logo-wrap
  size 120px
  absolute 50% false false 50%
  transform: translate(-50%, -50%);
  svg
    fill #ff0
  
.logo
.square
  size logo-size
  absolute 50% false false 50%
  margin auto
  transform-origin 85% -35%
  transform rotate(45deg) translate3d(0, 0, 0)
.logo
  background-color background-color
  z-index 1
  .inner
  .middle
  .outer
    absolute 50% false false 50%
    transform translate(-50%, -50%) translate3d(0, 0, 0)
  .inner
    size logo-inner-size
    .item-1
      top 0
      left 0
    .item-2
      top 0
      right 0
    .item-3
      bottom 0
      right 0
    .item-4
      bottom 0
      left 0
  .middle
    size logo-medium-size
    .item-1
      top 1px
      left 1px
    .item-2
      top 0
      left 11px
    .item-3
      top 0
      right 11px
    .item-4
      top 1px
      right 1px
    .item-5
      top 11px
      right 0
    .item-6
      bottom 11px
      right 0
    .item-7
      bottom 1px
      right 1px
    .item-8
      bottom 0
      right 11px
    .item-9
      bottom 0
      left 11px
    .item-10
      bottom 1px
      left 1px
    .item-11
      bottom 11px
      left 0
    .item-12
      top 11px
      left 0
  .outer
    size logo-outer-size
    .item-1
      top 1px
      left 1px
    .item-2
      top 1px
      left 12px
    .item-3
      top 0
      left 23px
    .item-4
      top 0
      right 23px
    .item-5
      top 1px
      right 12px
    .item-6
      top 1px
      right 1px
    .item-7
      top 12px
      right 1px
    .item-8
      top 23px
      right 0
    .item-9
      bottom 23px
      right 0
    .item-10
      bottom 12px
      right 1px
    .item-11
      bottom 1px
      right 1px
    .item-12
      bottom 1px
      right 12px
    .item-13
      bottom 0
      right 23px
    .item-14
      bottom 0
      left 23px
    .item-15
      bottom 1px
      left 12px
    .item-16
      bottom 1px
      left 1px
    .item-17
      bottom 12px
      left 1px
    .item-18
      bottom 23px
      left 0
    .item-19
      top 23px
      left 0
    .item-20
      top 12px
      left 1px
  .border
    size logo-size
    box-sizing border-box
    border logo-border-size solid logo-color
    opacity 0
.logo-point
  absolute false
  border-radius 50%
  background-color logo-color
  transform scale(0) translate3d(0, 0, 0)
  &.big
    size logo-point-big
  &.medium
    size logo-point-medium
  &.small
    size logo-point-small

.square
  z-index 0
  border 0px solid square-border-color
  // &.animate
  //   size 80vmax
  //   size 80vw
  //   opacity 0
  //   border-width 15px
  //   transition width square-transition-time linear,
  //              height square-transition-time linear,
  //              opacity square-transition-time linear,
  //              border-width square-transition-time linear

.ready
  .logo 
    .inner .logo-point
      animation scale-in logo-transition-time linear forwards
    .middle .logo-point
      animation scale-in logo-transition-time linear forwards logo-transition-time
    .outer .logo-point
      animation scale-in logo-transition-time linear forwards logo-transition-time * 2
    .border
      animation fade-in 0s linear forwards logo-transition-time * 3
  .square
    // animation square-start-scale square-transition-time normal linear logo-transition-time * 3,
    animation square-scale 5s infinite linear

  @keyframes scale-in
    0%
      transform scale(0)
    100%
      transform scale(1)
  @keyframes fade-in
    0%
      opacity 0
    100%
      opacity 1
  // @keyframes square-start-scale
  //   to
  //     size 80vmax
  //     size 80vw
  //     opacity 0
  //     border-width 15px
  //     transition width square-transition-time cubic-bezier(0.215, 0.61, 0.355, 1),
  //                height square-transition-time cubic-bezier(0.215, 0.61, 0.355, 1),
  //                opacity square-transition-time cubic-bezier(0.215, 0.61, 0.355, 1),
  //                border-width square-transition-time cubic-bezier(0.215, 0.61, 0.355, 1)
  @keyframes square-scale
    18%
      opacity 1
      size logo-size
      border-width 0
      transition width 0 linear,
                 height 0 linear,
                 opacity 0 linear,
                 border-width 0 linear
    44%
      size 80vmax
      size 80vw
      opacity 0
      border-width 15px
      transition width square-transition-time linear,
                 height square-transition-time linear,
                 opacity square-transition-time linear,
                 border-width square-transition-time linear
    44.1%
      opacity 1
      size logo-size
      border-width 0
      transition width 0 linear,
                 height 0 linear,
                 opacity 0 linear,
                 border-width 0 linear
      